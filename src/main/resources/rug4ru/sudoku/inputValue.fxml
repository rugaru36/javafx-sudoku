<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" fx:id="vbox" spacing="20.0"
  xmlns="http://javafx.com/javafx/8.0.171"
  xmlns:fx="http://javafx.com/fxml/1" fx:controller="rug4ru.sudoku.presentation.controllers.InputValueController">
  <children>
    <Label fx:id="messageLabel" text="Input Value" />

    <TextField fx:id="valueTextField" alignment="CENTER"/>
    <Button fx:id="okButton" text="Ok" onAction="#onOk"/>
  </children>
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
</VBox>
